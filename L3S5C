public class Calculator {

  // Overload add method
  public double add(double a, double b) {
    return a + b;
  }

  public double add(double a, double b, double c) {
    return a + b + c;
  }

  // Overload subtract 
  public double subtract(double a, double b) {
    return a - b;
  }

  // Overload multiply
  public double multiply(double a, double b) {
    return a * b;
  }

}
